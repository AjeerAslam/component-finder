{
  "name": "frontend-markup-searcher",
  "version": "1.0.0",
  "description": "A tool to search markup found in frontend services",
  "main": "index.js",
  "scripts": {
    "test": "nyc ava test/**/*.js --verbose"
  },
  "engines": {
    "node": ">=6.0"
  },
  "author": "Ben Chidgey",
  "license": "Apache 2.0 License",
  "dependencies": {
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-register": "^6.11.6",
    "co": "^4.6.0",
    "nock": "^8.0.0",
    "progress": "^1.1.8"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "lib/**/*.js",
      "finder.js"
    ],
    "require": [
      "babel-register"
    ]
  }
}
